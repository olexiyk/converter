{
    "name": "researchgate/converter",
    "description": "A small library to convert different bibliography formats into each other. Forked from 'geissler/converter'.",
    "keywords": ["csl", "bibtex", "endnote", "ris", "bibliography"],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/researchgate/converter",
    "authors":[
        {
            "name":"Benjamin Geißler",
            "email":"benjamin.geissler@gmail.com"
        },
        {
            "name": "Níckolas Da Silva",
            "email": "nickolas@phpsp.org.br"
        }
    ],
    "require":{
        "php": ">=7.3.0",
        "ext-json": "*",
        "technosophos/libris": "dev-master"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.3",
        "squizlabs/php_codesniffer": "^3.5",
        "vimeo/psalm": "^3.16",
        "phpstan/phpstan": "^0.12.42"
    },
    "autoload":{
        "psr-4":{
            "Geissler\\Converter\\": "src/"
        },
        "files": [
            "library/bibtexParse/PARSEENTRIES.php",
            "library/bibtexParse/PARSECREATORS.php",
            "library/bibtexParse/PARSEMONTH.php",
            "library/bibtexParse/PARSEPAGE.php"
        ]
    }
}
